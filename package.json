{
  "name": "ngx-seed",
  "version": "1.0.1",
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./set-env.ts",
    "start": "cross-env API_BASE_URL=http://localhost:8080 npm run config && ng serve",
    "build": "npm run config && ng build --prod --aot",
    "build-local": "cross-env PRODUCTION=true API_BASE_URL=http://localhost:8080 npm run config && ng build --prod --aot",
    "test": "ng test --code-coverage",
    "test-ci": "ng test --code-coverage --no-watch --browsers ChromeHeadless",
    "lint": "ng lint",
    "e2e": "cross-env API_BASE_URL=http://localhost:8080 npm run config && ng e2e",
    "ci": "cross-env CI_MODE=true npm run config && ng e2e && npm run test-ci && npm run build",
    "ci-local": "cross-env API_BASE_URL=http://localhost:8080 CI_MODE=true npm run config && ng e2e && npm run test-ci && npm run build-local"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.10",
    "@angular/cdk": "^7.3.5",
    "@angular/common": "^7.2.10",
    "@angular/compiler": "^7.2.10",
    "@angular/core": "^7.2.10",
    "@angular/forms": "^7.2.10",
    "@angular/http": "^7.2.10",
    "@angular/material": "^7.3.5",
    "@angular/platform-browser": "^7.2.10",
    "@angular/platform-browser-dynamic": "^7.2.10",
    "@angular/router": "^7.2.10",
    "@netbasal/spectator": "^3.6.0",
    "@ngx-translate/core": "^11.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "core-js": "^2.6.5",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.3.3",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.5",
    "@angular/cli": "^7.3.6",
    "@angular/compiler-cli": "^7.2.10",
    "@angular/language-service": "^7.2.10",
    "@netbasal/spectator": "~3.2.1",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "cross-env": "^5.2.0",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.3",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "protractor": "~5.4.2",
    "protractor-jasmine2-screenshot-reporter": "^0.5.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.0"
  }
}
